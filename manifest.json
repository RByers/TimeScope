{
  "manifest_version": 3,
  "name": "TimeScope",
  "version": "1.0.0",
  "description": "A Chrome extension to track time spent by activity",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "dist/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "TimeScope"
  }
}
